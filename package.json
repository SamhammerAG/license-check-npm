{
  "name": "npm-license-check-action",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:SamhammerAG/npm-license-check-action.git",
  "author": "Roland Schmitt <rschmitt@samhammer.de>",
  "license": "MIT",
  "scripts": {
    "build": "ncc build src/main.js",
    "run-valid": "node src/main.js --path . --licenseWhitelist MIT;BSD-3-Clause",
    "run-invalid": "node src/main.js --path . --licenseWhitelist NONE",
    "run-empty": "node src/main.js --path ."
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "license-checker-rseidelsohn": "^3.3.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.7"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1"
  }
}
